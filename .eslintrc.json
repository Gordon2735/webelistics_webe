{
  "env": {
    "node": true,
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "rules": {
    "plugins": ["html", "handlebars"],
    "prettier.singleQuote": true,
    "prettier.tabWidth": 4,
    "prettier.useTabs": true
  }
}
